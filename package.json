{
	"name": "csrdelft-handlebars",
	"version": "1.0.0",
	"main": "index.html",
	"scripts": {
		"start": "vite",
		"build": "vite build",
		"serve": "vite preview",
		"css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
		"css-prefix": "postcss --replace assets/css/*.css --use autoprefixer --map",
		"css": "npm-run-all css-compile css-prefix"
	},
	"author": "Sebastiaan Benjamins",
	"license": "MIT",
	"private": true,
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"@popperjs/core": "^2.10.2",
		"bootstrap": "^5.1.3",
		"jquery": "^3.6.0"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.0",
		"handlebars": "^4.7.7",
		"node-sass": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.3.11",
		"postcss-cli": "^9.0.2",
		"purgecss": "^4.0.3",
		"vite": "^2.6.14",
		"vite-plugin-handlebars": "^1.6.0"
	}
}
